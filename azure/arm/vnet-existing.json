{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "NetworkName": {
            "type": "string"
        },
        "NetworkAddressPrefix": {
            "type": "string",
            "metadata": {
                "description": "CIDR for Virtual Network, e.g., 1.2.3.4/16"
            }
        },
        "SubnetName": {
            "type": "string"
        },
        "SubnetAddressPrefix": {
            "type": "string"
        },
        "SubnetStartAddress": {
            "type": "string"
        },
        "ResourceGroupName": {
            "type": "string",
            "metadata": {
                "description": "the RG which owns the existing VNET"
            }
        },
        "Location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]"
        },
        "Neo4Jtags": {
            "type": "object",
            "metadata": {
                "description": "Dictionary of tags to apply to the VNET"
            }
        },
        "ArtifactsBase": {
            "type": "string",
            "metadata": {
                "artifactsBaseUrl": ""
            },
            "defaultValue": "https://s3.amazonaws.com/neo4j-arm/test"
        }
    },
    "variables": {},
    "resources": [],
    "outputs": {
        "vnetResourceID": {
            "type": "string",
            "value": "[resourceID(parameters('ResourceGroupName'), 'Microsoft.Network/virtualNetworks', parameters('NetworkName'))]"
        }
    }
}
